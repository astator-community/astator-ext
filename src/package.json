{
	"name": "astator-ext",
	"displayName": "astator-ext",
	"icon": "assets/icon.png",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:csharp",
		"onCommand:astator-ext.createProject",
		"onCommand:astator-ext.openCommands"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "astator-ext.openCommands",
				"title": "astator: 命令列表",
				"icon": "assets/icon.png"
			},
			{
				"command": "astator-ext.runProject",
				"title": "astator: 运行项目"
			},
			{
				"command": "astator-ext.runScript",
				"title": "astator: 运行脚本"
			},
			{
				"command": "astator-ext.saveProject",
				"title": "astator: 保存项目"
			},
			{
				"command": "astator-ext.connect",
				"title": "astator: 连接设备"
			},
			{
				"command": "astator-ext.createProject",
				"title": "astator: 新建项目"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == csharp || resourceLangId == csproj || resourceLangId == xml",
					"command": "astator-ext.openCommands",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/adm-zip": "^0.4.34",
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@lvgithub/stick": "^3.1.11",
		"adm-zip": "^0.5.9",
		"fs-extra": "^10.0.0"
	}
}